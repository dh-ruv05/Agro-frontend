{
    "version": 2,
    "builds": [
        {
            "src": "server.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.py"
        }
    ],
    "env": {
        "BACKEND_API_URL": "https://your-backend-url.railway.app"
    }
} 